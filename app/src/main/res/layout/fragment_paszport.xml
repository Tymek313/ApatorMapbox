<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             xmlns:app="http://schemas.android.com/apk/res-auto"
             xmlns:tools="http://schemas.android.com/tools"
             android:layout_width="match_parent"
             android:layout_height="match_parent"
             tools:context=".fragments.PassportFragment">
    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <androidx.core.widget.NestedScrollView
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:fillViewport="true"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:id="@+id/nestedScrollView">
            <androidx.constraintlayout.widget.ConstraintLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content">

                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="250dp"
                        android:id="@+id/poa_Layout"
                        app:layout_constraintTop_toBottomOf="@+id/constraintLayout"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent">
                    <com.anychart.AnyChartView
                            android:id="@+id/poa_monthly_ACV_PF"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="8dp"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="8dp"/>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="250dp"
                        android:id="@+id/dc_Layout"
                        app:layout_constraintTop_toBottomOf="@+id/poa_Layout"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent">
                    <com.anychart.AnyChartView
                            android:id="@+id/dc_monthly_ACV_PF"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="8dp"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="8dp"/>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="250dp"
                        android:id="@+id/ac_Layout"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/dc_Layout">
                    <com.anychart.AnyChartView
                            android:id="@+id/ac_monthly_ACV_PF"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="8dp"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="8dp"/>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="match_parent"
                        android:layout_height="250dp"
                        android:id="@+id/solrad_Layout"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toBottomOf="@+id/ac_Layout">
                    <com.anychart.AnyChartView
                            android:id="@+id/solrad_monthly_ACV_PF"
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="8dp"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            app:layout_constraintEnd_toEndOf="parent"
                            android:layout_marginEnd="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="8dp"/>

                </androidx.constraintlayout.widget.ConstraintLayout>
                <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="0dp"
                        android:layout_height="wrap_content"
                        android:id="@+id/constraintLayout"
                        app:layout_constraintStart_toStartOf="parent"
                        app:layout_constraintEnd_toEndOf="parent"
                        app:layout_constraintTop_toTopOf="parent">
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/distance_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/location_value_PF"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/state_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/distance_value_PF"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/city_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/state_value_PF"
                    />
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/tz_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/elev_value_PF"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/location_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/tz_value_PF"
                    />
                    <TextView
                            android:text="@string/lat_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/lat_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"/>
                    <TextView
                            android:text="@string/Lon_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/lon_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/lat_textView_PF"/>
                    <TextView
                            android:text="@string/elev_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/elev_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/lon_textView_PF"/>
                    <TextView
                            android:text="@string/tz_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/tz_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/elev_textView_PF"/>
                    <TextView
                            android:text="@string/location_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/location_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/tz_textView_PF"
                    />
                    <TextView
                            android:text="@string/City_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/city_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/state_textView_PF"
                    />
                    <TextView
                            android:text="@string/state_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/state_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/distance_textView_PF"/>
                    <TextView
                            android:text="@string/Distance_textView_PF"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/distance_textView_PF"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/location_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/lat_value_PF"
                            app:layout_constraintTop_toTopOf="parent"
                            android:layout_marginTop="8dp"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/lon_value_PF"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/lat_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/elev_value_PF"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/lon_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"/>
                    <androidx.constraintlayout.widget.Guideline android:layout_width="wrap_content"
                                                                android:layout_height="wrap_content"
                                                                android:id="@+id/guideline"
                                                                app:layout_constraintGuide_begin="205dp"
                                                                android:orientation="vertical"/>
                    <TextView
                            android:text="Ac Annual"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/ac_annual_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/city_textView_PF"/>
                    <TextView
                            android:text="Solrad Annual"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/solrad_annual_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="105dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/ac_annual_textView_PF"/>
                    <TextView
                            android:text="Capacity Factor"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/capacity_factor_textView_PF"
                            app:layout_constraintStart_toStartOf="parent"
                            android:layout_marginStart="104dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/solrad_annual_textView_PF"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="8dp"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/ac_annual_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/city_value_PF"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/solrad_annual_value_PF"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/ac_annual_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"/>
                    <TextView
                            android:text="@string/value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:id="@+id/capacity_factor_value_PF"
                            app:layout_constraintStart_toStartOf="@+id/guideline"
                            android:layout_marginStart="8dp"
                            app:layout_constraintBottom_toBottomOf="parent"
                            android:layout_marginBottom="8dp"
                            android:layout_marginTop="8dp"
                            app:layout_constraintTop_toBottomOf="@+id/solrad_annual_value_PF"/>
                </androidx.constraintlayout.widget.ConstraintLayout>
            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.core.widget.NestedScrollView>
        <Button
                android:text="@string/back_btn_PF"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/back_btn_PF"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginBottom="24dp"
                android:drawableLeft="@drawable/ic_arrow_back"
                android:layout_marginEnd="32dp"
                app:layout_constraintEnd_toEndOf="parent"
                android:textSize="14sp"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</FrameLayout>