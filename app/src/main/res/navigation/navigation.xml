<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/navigation"
            app:startDestination="@id/mapFragment">

    <fragment android:id="@+id/mapFragment"
              android:name="com.example.apatormapbox.fragments.MapFragment"
              android:label="fragment_map"
              tools:layout="@layout/fragment_map">
        <action android:id="@+id/action_mapFragment_to_settingsFragment"
                app:destination="@id/settingsFragment"/>
    </fragment>
    <fragment android:id="@+id/settingsFragment"
              android:name="com.example.apatormapbox.SettingsFragment"
              android:label="SettingsFragment"/>
</navigation>